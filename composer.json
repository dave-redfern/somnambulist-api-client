{
    "name": "somnambulist/api-client",
    "type": "library",
    "description": "A library to help ease creating API integrations",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-json":  "*",
        "beberlei/assert": "^3.0",
        "psr/log": "^1.0",
        "pagerfanta/pagerfanta": "^2.4",
        "somnambulist/attribute-model": "^2.0",
        "somnambulist/collection": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/routing": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "somnambulist/domain": "^4.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Components\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Components\\ApiClient\\Tests\\": "tests/"
        }
    }
}
